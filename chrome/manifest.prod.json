{
  "version": "0.0.0",
  "name": "ipfs-station",
  "manifest_version": 2,
  "description": "IPFS station",
  "browser_action": {
    "default_title": "ipfs-station",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png",
    "scalable": "icon-large.svg"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "http://localhost/"
  ],
  "content_security_policy": "default-src 'self'; script-src 'self'; connect-src http://localhost:5001; style-src * 'unsafe-inline'; img-src 'self' data:;"
}
