    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "chromedriver": "^2.19.0",
    "gulp": "^3.9.0",
    "gulp-jade": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-util": "^3.0.5",
    "gulp-zip": "^3.0.2",
    "jsdom": "^6.5.1",
    "pre-commit": "^1.1.2",
    "raw-loader": "^0.5.1",
